<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ab.hibernateBasic.Student" table="STUDS">
        <id name="id" type="int">  <column name="ID" />  </id>
        <property name="name" type="java.lang.String">  <column name="NAME" />  </property>
        <property name="surname" type="java.lang.String">  <column name="SURNAME" />  </property>
        <property name="age" type="int">  <column name="AGE" />  </property>
    </class>
</hibernate-mapping>